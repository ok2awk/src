"""

## Testing Stats

"""

@include "oklib"
@include "stats"

function _norm(f, n,m,h) {
	print("")
	Normal(n,10,1)
	Hist(h,5,15,10)
	m=1000
	while(m-- > 0)
		Hist1(h, Normal1(n))	
	Histshow(h)
}
function _beta(f,  r,h) {
	print("")
	Hist(h,0,1,10)
	r=1000
	while(r-- > 0)
		Hist1(h, beta80())
	Histshow(h)
}
BEGIN {
  tests("_norm,_beta")
}
