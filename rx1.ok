"""

## Testing Tiles

"""

@include "oklib"
@include "rx"

function _xtiles(f,       pre,k,r,r0,rxs,lst) {
	r0     = 100
	pre[1] = 0.5
	pre[2] = 2
	pre[3] = 1
	pre[4] = 2
	for(k in pre) {
		haves(rxs, k, "Rx", "rx" k)
		r=r0
		empty(lst)
		while(r-- > 0)
			push(lst, k+rand()^pre[k])
		Rx1(rxs[k], lst)
	}
}
BEGIN {
  tests("_xtiles")
}

